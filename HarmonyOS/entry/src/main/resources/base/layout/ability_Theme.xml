<?xml version="1.0" encoding="utf-8"?>

    <DependentLayout
        xmlns:ohos="http://schemas.huawei.com/res/ohos"
        ohos:height="match_content"
        ohos:width="match_content"
        ohos:background_element="#FFEFEFEF"
        ohos:padding="15vp">
        <Button
            ohos:id="$+id:theme_1"
            ohos:align_parent_left="true"
            ohos:height="150px"
            ohos:width="128px"
            ohos:margin="6vp"
            ohos:background_element="$graphic:Theme1"
            ohos:layout_alignment="center"
            />
        <Button
            ohos:id="$+id:theme_2"
            ohos:right_of="$+id:theme_1"
            ohos:height="150px"
            ohos:width="128px"
            ohos:margin="6vp"
            ohos:background_element="$graphic:Theme2"
            ohos:layout_alignment="center"
            />
        <Button
            ohos:id="$+id:theme_3"
            ohos:right_of="$+id:theme_2"
            ohos:height="150px"
            ohos:width="128px"
            ohos:margin="6vp"
            ohos:background_element="$graphic:Theme3"
            ohos:layout_alignment="center"
            />
        <Button
            ohos:id="$+id:theme_4"
            ohos:top_margin="20vp"
            ohos:below="$+id:theme_1"
            ohos:align_parent_left="true"
            ohos:height="150px"
            ohos:width="128px"
            ohos:margin="6vp"
            ohos:background_element="$graphic:Theme4"
            ohos:layout_alignment="center"
            />
        <Button
            ohos:id="$+id:theme_5"
            ohos:below="$+id:theme_2"
            ohos:right_of="$+id:theme_4"
            ohos:top_margin="20vp"
            ohos:height="150px"
            ohos:width="128px"
            ohos:margin="6vp"
            ohos:background_element="$graphic:Theme5"
            ohos:layout_alignment="center"
            />
        <Button
            ohos:id="$+id:theme_6"
            ohos:below="$+id:theme_3"
            ohos:right_of="$+id:theme_5"
            ohos:top_margin="20vp"
            ohos:height="150px"
            ohos:width="128px"
            ohos:margin="6vp"
            ohos:background_element="$graphic:Theme6"
            ohos:layout_alignment="center"
            />
    </DependentLayout>