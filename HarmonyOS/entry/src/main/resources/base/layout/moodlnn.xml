<?xml version="1.0" encoding="utf-8"?>
<DirectionalLayout
    ohos:id="$+id:mine_dircontent"
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="match_parent"
    ohos:width="match_parent"
    ohos:orientation="vertical"
    ohos:background_element="$media:moodlnnbackground">
    <Button
        ohos:id="$+id:moodlnn_back"
        ohos:height="128px"
        ohos:width="128px"
        ohos:background_element="$media:pageback"
        ohos:margin="10vp"
        ohos:layout_alignment="left"

        />
<DirectionalLayout
    xmlns:ohos="http://schemas.huawei.com/res/ohos"
    ohos:height="1300px"
    ohos:width="1080px"
    ohos:orientation="horizontal"
    ohos:alignment="center"
    ohos:padding="30vp"
    ohos:top_margin="150vp"
    ohos:background_element="$media:moodkuang"
    >
    <ListContainer
        ohos:id="$+id:moodlnnlistcontainer"
        ohos:height="1150px"
        ohos:width="1080px"
        ohos:orientation="vertical"
        ohos:background_element="#00D72626"/>
</DirectionalLayout>
</DirectionalLayout>